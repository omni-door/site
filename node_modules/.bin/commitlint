#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/han/Desktop/GitHub/omni-site/node_modules/.pnpm/@commitlint+cli@8.3.5/node_modules/@commitlint/cli/lib/node_modules:/Users/han/Desktop/GitHub/omni-site/node_modules/.pnpm/@commitlint+cli@8.3.5/node_modules/@commitlint/cli/node_modules:/Users/han/Desktop/GitHub/omni-site/node_modules/.pnpm/@commitlint+cli@8.3.5/node_modules/@commitlint/node_modules:/Users/han/Desktop/GitHub/omni-site/node_modules/.pnpm/@commitlint+cli@8.3.5/node_modules:/Users/han/Desktop/GitHub/omni-site/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/han/Desktop/GitHub/omni-site/node_modules/.pnpm/@commitlint+cli@8.3.5/node_modules/@commitlint/cli/lib/node_modules:/Users/han/Desktop/GitHub/omni-site/node_modules/.pnpm/@commitlint+cli@8.3.5/node_modules/@commitlint/cli/node_modules:/Users/han/Desktop/GitHub/omni-site/node_modules/.pnpm/@commitlint+cli@8.3.5/node_modules/@commitlint/node_modules:/Users/han/Desktop/GitHub/omni-site/node_modules/.pnpm/@commitlint+cli@8.3.5/node_modules:/Users/han/Desktop/GitHub/omni-site/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@commitlint/cli/lib/cli.js" "$@"
else
  exec node  "$basedir/../@commitlint/cli/lib/cli.js" "$@"
fi
